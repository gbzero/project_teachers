<% if flash[:notice] %>
  <div class="alert alert-dismissable alert-success">
    <%= notice %>
  </div>
<% end %>

<p>
  <strong>Sender:</strong>
  <%= @invitation.sender_id %>
</p>

<p>
  <strong>Recipient email:</strong>
  <%= @invitation.recipient_email %>
</p>

<p>
  <strong>Token:</strong>
  <%= @invitation.token %>
</p>

<p>
  <strong>Sent at:</strong>
  <%= @invitation.sent_at %>
</p>

<p>
  <strong>New:</strong>
  <%= @invitation.new %>
</p>

<%= link_to 'Editar', edit_invitation_path(@invitation) %> |
<%= link_to 'Volver', invitations_path %>
